{
  "name": "@eonian/upgradeable",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "clear": "rm -rf dist",
    "dev": "concurrently -c \"auto\" \"yarn:test:watch\" \"yarn:build:watch\"",
    "rebuild": "yarn clear && yarn build",
    "build": "microbundle",
    "build:watch": "microbundle watch",
    "prepublish": "yarn build",
    "lint-ts": "eslint . --ext .ts --fix",
    "pre-commit": "lint-staged"
  },
  "source": "./src/index.ts",
  "exports": {
    "default": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.5.3",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "concurrently": "^7.6.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^5.7.2",
    "hardhat": "^2.13.0",
    "hardhat-deploy": "0.11.36",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-tracer": "^2.5.1",
    "jest": "^29.6.1",
    "lint-staged": "^14.0.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.2",
    "prettier-plugin-solidity": "^1.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@uniswap/token-lists": "^1.0.0-beta.33"
  },
  "lint-staged": {
    "*.ts": "yarn lint-ts"
  }
}