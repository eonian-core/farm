{
    "name": "@eonian/upgradeable",
    "version": "0.2.0",
    "license": "MIT",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "clear": "rm -rf dist",
        "dev": "concurrently -c \"auto\" \"yarn:test:watch\" \"yarn:build:watch\"",
        "rebuild": "yarn clear && yarn build",
        "build": "microbundle --target=node",
        "build:watch": "microbundle watch --target=node",
        "prepublish": "yarn rebuild",
        "lint": "eslint . --ext .ts,.js --fix",
        "test:hardhat:deploy": "HARDHAT_FORK_NETWORK=false hardhat test test/deploy/*.test.*"
    },
    "source": "./src/index.ts",
    "exports": {
        "default": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.module.js",
    "unpkg": "./dist/index.umd.js",
    "types": "./dist/index.d.ts",
    "devDependencies": {
        "@eonian/eslint-config": "^0.0.7",
        "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
        "@typechain/ethers-v6": "^0.5.1",
        "@typechain/hardhat": "^9.1.0",
        "@types/debug": "^4.1.12",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.11.30",
        "concurrently": "^7.6.0",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "jest": "^29.6.1",
        "microbundle": "^0.15.1",
        "prettier": "^3.0.2",
        "prettier-plugin-solidity": "^1.1.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typechain": "^8.3.2",
        "typescript": "^5.4.2"
    },
    "dependencies": {
        "@chainlink/contracts": "^0.8.0",
        "@nomicfoundation/hardhat-foundry": "^1.1.1",
        "@nomicfoundation/hardhat-ethers": "^3.0.6",
        "@nomicfoundation/hardhat-verify": "^2.0.5",
        "@openzeppelin/contracts": "^4.9.6",
        "@openzeppelin/contracts-upgradeable": "^4.9.6",
        "@openzeppelin/hardhat-upgrades": "^3.2.0",
        "@openzeppelin/upgrades-core": "^1.32.0",
        "@types/sinon": "^10.0.15",
        "lodash": "^4.17.21",
        "sinon": "^15.1.0",
        "tree-kill": "^1.2.2",
        "ethers": "^6.13.1",
        "@uniswap/token-lists": "^1.0.0-beta.34",
        "hardhat": "2.19.5",
        "hardhat-docgen": "^1.3.0",
        "hardhat-gas-reporter": "^2.0.2",
        "hardhat-tracer": "^2.8.1",
        "debug": "^4.3.4"
    },
    "peerDependencies": {
        "@chainlink/contracts": "^0.8.0",
        "@nomicfoundation/hardhat-foundry": "^1.1.1",
        "@nomicfoundation/hardhat-verify": "^2.0.5",
        "@openzeppelin/contracts": "^4.9.6",
        "@openzeppelin/contracts-upgradeable": "^4.9.6",
        "@openzeppelin/upgrades-core": "^1.32.0",
        "@types/sinon": "^10.0.15",
        "lodash": "^4.17.21",
        "sinon": "^15.1.0",
        "tree-kill": "^1.2.2",
        "@uniswap/token-lists": "^1.0.0-beta.34",
