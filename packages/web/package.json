{
  "name": "@eonian/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -c \"auto\" \"yarn:dev:next\" \"yarn:storybook\" \"yarn:open:next\" \"yarn:mock\" ",
    "dev:next-mock": "concurrently -c \"auto\" \"yarn:dev:next\" \"yarn:mock\" ",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006 --quiet --no-open",
    "build:storybook": "storybook build",
    "serve:storybook": "serve ./storybook-static",
    "production": "next build && next start",
    "open:next": "opener http://localhost:3000",
    "test": "yarn jest --watchAll",
    "test:ci": "yarn jest --ci",
    "jest": "node --experimental-vm-modules --experimental-specifier-resolution=node ./node_modules/.bin/jest",
    "mock": "ts-node --esm ./mock.ts",
    "gen:gql-types": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0-alpha.15",
    "@apollo/experimental-nextjs-app-support": "^0.1.0",
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@next/mdx": "13.4.4",
    "@nextui-org/react": "^1.0.0-beta.13",
    "@radix-ui/react-collapsible": "^1.0.2",
    "@react-spring/web": "^9.6.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@svgdotjs/svg.js": "^3.1.2",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.9",
    "@types/react-transition-group": "^4.4.5",
    "@web3-onboard/core": "^2.16.2",
    "@web3-onboard/injected-wallets": "^2.8.4",
    "@web3-onboard/react": "^2.7.2",
    "@web3-onboard/walletconnect": "^2.3.5",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "eslint": "8.30.0",
    "eslint-config-next": "^13.4.1",
    "framer-motion": "^8.1.9",
    "graphql": "^16.6.0",
    "lodash.debounce": "^4.0.8",
    "next": "^13.4.4",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.20",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "react-transition-group": "^4.4.5",
    "react-wrap-balancer": "^0.4.0",
    "redux": "^4.2.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@apollo/server": "^4.6.0",
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/client-preset": "3.0.0",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-resolvers": "^3.2.0",
    "@graphql-tools/graphql-file-loader": "^7.5.16",
    "@graphql-tools/load": "^7.8.13",
    "@graphql-tools/mock": "^8.7.19",
    "@graphql-tools/schema": "^9.0.17",
    "@storybook/addon-essentials": "7.0.2",
    "@storybook/addon-interactions": "7.0.2",
    "@storybook/addon-links": "7.0.2",
    "@storybook/blocks": "7.0.2",
    "@storybook/nextjs": "7.0.2",
    "@storybook/react": "7.0.2",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash.debounce": "^4.0.7",
    "@types/testing-library__jest-dom": "^5.14.5",
    "concurrently": "^7.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "graphql-scalars": "^1.21.3",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "opener": "^1.5.2",
    "prettier": "^2.8.7",
    "rehype-external-links": "^2.0.1",
    "sass": "^1.60.0",
    "serve": "^14.2.0",
    "storybook": "7.0.2",
    "storybook-addon-pseudo-states": "^2.0.0-next.0",
    "ts-node": "^10.9.1"
  }
}
